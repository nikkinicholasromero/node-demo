{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "always"],
    "quotemark": [true, "double"],
    "no-unused-variable": true,
    "interface-name": [true, "never-prefix"],
    "member-access": [true, "no-public"],
    "trailing-comma": [true, "always"]
  },
  "rulesDirectory": []
}
